    "@types/webpack": "^4.41.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "ts-loader": "^6.2.1"
  }

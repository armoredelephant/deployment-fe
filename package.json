    "@types/react-dom": "^16.9.4",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "apollo-boost": "^0.4.7",
    "formik": "^2.0.8",
    "graphql": "^14.5.8",
